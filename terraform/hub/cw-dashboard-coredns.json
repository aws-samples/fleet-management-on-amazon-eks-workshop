
{
    "variables": [
        {
            "type": "property",
            "property": "ClusterName",
            "inputType": "select",
            "id": "ClusterName",
            "label": "Cluster name",
            "defaultValue": "**default_cluster**",
            "visible": true,
            "search": "{ContainerInsights/Prometheus,ClusterName} MetricName=coredns_dns_requests_total",
            "populateFrom": "ClusterName"
        }
    ],

   
"widgets": [
    {
        "height": 6,
        "width": 7,
        "y": 0,
        "x": 7,
        "type": "metric",
        "properties": {
            "metrics": [
                [ { "expression": "100*m1/m2", "label": "eniUtilization", "id": "e1" } ],
                [ { "expression": "AVG(SEARCH('{Kubernetes,CLUSTER_ID} MetricName=\"eniAllocated\" CLUSTER_ID=\"**default_cluster**\"', 'Average', 300))", "id": "m1", "visible": false } ],
                [ { "expression": "AVG(SEARCH('{Kubernetes,CLUSTER_ID} MetricName=\"eniMaxAvailable\" CLUSTER_ID=\"**default_cluster**\"', 'Average', 300))", "id": "m2", "visible": false } ]
            ],
            "view": "gauge",
            "stacked": false,
            "region": "**aws_region**",
            "stat": "Sum",
            "period": 300,
            "yAxis": {
                "left": {
                    "min": 0,
                    "max": 100
                }
            },
            "title": "Cache hit percentage"
        }
    },
        {
            "height": 3,
            "width": 6,
            "y": 3,
            "x": 14,
            "type": "metric",
            "properties": {
                "metrics": [
                    [ { "expression": "SELECT SUM(coredns_cache_entries) FROM \"ContainerInsights/Prometheus\" WHERE ClusterName = '${ClusterName}'" } ]
                ],
                "sparkline": true,
                "view": "singleValue",
                "region": "**aws_region**",
                "stat": "Sum",
                "period": 300,
                "stacked": false,
                "setPeriodToTimeRange": true,
                "title": "Cache entries"
            }
        },
        {
            "height": 3,
            "width": 6,
            "y": 0,
            "x": 14,
            "type": "metric",
            "properties": {
                "metrics": [
                    [ { "expression": "SELECT SUM(coredns_dns_requests_total) FROM \"ContainerInsights/Prometheus\" WHERE ClusterName = '${ClusterName}'" } ]
                ],
                "view": "singleValue",
                "stacked": false,
                "region": "**aws_region**",
                "stat": "Sum",
                "period": 300,
                "title": "Requests (total for last 5 mins)"
            }
        },
        {
            "height": 6,
            "width": 7,
            "y": 0,
            "x": 0,
            "type": "metric",
            "properties": {
                "metrics": [
                    [ { "expression": "SELECT SUM(coredns_dns_requests_total) FROM \"ContainerInsights/Prometheus\" WHERE ClusterName = '${ClusterName}'" } ]
                ],
                "view": "timeSeries",
                "stacked": false,
                "region": "**aws_region**",
                "title": "CoreDNS requests total",
                "period": 300,
                "stat": "Sum"
            }
        }
    ]
}
